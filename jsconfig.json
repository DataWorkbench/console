{
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist"],
  "compilerOptions": {
    "jsx": "react",
    "baseUrl": "./src",
    "paths": {
      "components/*": ["components/*"],
      "views/*": ["views/*"],
      "stores/*": ["stores/*"],
      "scss/*": ["scss/*"],
      "hooks/*": ["hooks/*"],
      "utils/*": ["utils/*"]
    }
  }
}
